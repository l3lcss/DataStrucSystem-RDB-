{
  "name": "DataStrucSystem",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "git log | head -1 | sed s/'commit '// | cut -c1-7 > ./static/version.txt && echo window.webVersion = \\'$(git log | head -1 | sed s/'commit '// | cut -c1-7)\\' > ./web_version.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "buefy": "^0.6.7",
    "firebase": "^5.5.0",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.24.0",
    "scrollreveal": "^4.0.4",
    "vue": "^2.5.17",
    "vue-carousel": "^0.14.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.2.4",
    "vuex": "^3.0.1",
    "vuexfire": "^2.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.3",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
